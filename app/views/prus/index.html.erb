<p id="notice"><%= notice %></p>

<h1>Prus</h1>

<table>
  <thead>
    <tr>
      <th>Foto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prus.each do |pru| %>
      <tr>
        <td><%= pru.foto %></td>
        <td><%= link_to 'Show', pru %></td>
        <td><%= link_to 'Edit', edit_pru_path(pru) %></td>
        <td><%= link_to 'Destroy', pru, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pru', new_pru_path %>
